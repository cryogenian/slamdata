        "port": 63174
    },
    "restoreCmd": "mongorestore --host 127.0.0.1 -d testDb --port 63174 --drop test/dump/demo",
    "selenium": {
        "jar": "test/jars/selenium-server-standalone-2.53.0.jar",
        "waitTime": 30000
    },
    "sauceLabs": {
        "enabled": false,
        "platform": "OS X 10.10",
        "maxDuration": 10800
    },
    "slamdataUrl": "http://localhost:63175",
    "quasar": {
        "jar": "jars/quasar.jar",
        "config": "test/quasar-config.json"
    },
    "database": {
        "name": "testDb"
    },
    "upload": {
        "filePaths": ["./test/line-delimited.json", "./test/array-wrapped.json"]
    },
    "download": {
